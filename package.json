{
  "name": "@vgvenkat/micro-lib",
  "version": "0.0.0-semantically-released",
  "description": "Random start wars names micro-lib",
  "main": "src/index.js",
  "scripts": {
    "precommit": "npm test && npm run check-coverage",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage":"cat ./coverage/lcov.info | codecov",
    "commit": "git-cz",
    "test": "istanbul cover -x **/*.test.js _mocha -- -R spec src/index.test.js",
    "watch": "mocha src/index.test.js -w",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vgvenkat/starwars-names.git"
  },
  "keywords": [
    "random",
    "star",
    "wars"
  ],
  "author": "vgvenkat <vgkrish619@gmail.com> (http://vgvenkataraman.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vgvenkat/starwars-names/issues"
  },
  "homepage": "https://github.com/vgvenkat/starwars-names#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.8.6",
    "cz-conventional-changelog": "1.2.0",
    "ghooks": "1.3.2",
    "husky": "0.11.9",
    "istanbul": "0.4.5",
    "mocha": "3.1.2",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
